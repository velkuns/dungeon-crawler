{
  "name": "eureka/eureka-appplication-web",
  "description": "Web Application Eureka Framework - Template App",
  "minimum-stability": "stable",
  "type": "project",
  "license": "MIT",

  "authors": [
    {
      "name": "Romain Cottard",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Application\\": "./src"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Application\\Tests\\Unit\\": "./tests/unit"
    }
  },

  "config": {
    "sort-packages": true
  },

  "require": {
    "php": "8.1.*||8.2.*||8.3.*",
    "ext-json": "*",
    "ext-pdo": "*",

    "eureka/component-console": "^6.1.0",
    "eureka/component-database": "^3.3.0",
    "eureka/component-deployer": "^3.0.0",
    "eureka/component-orm": "^6.0.0",
    "eureka/component-password": "^5.0.0",
    "eureka/component-validation": "^5.3.0",
    "eureka/component-web": "^6.0.0",
    "eureka/kernel-http": "^6.0.0",
    "eureka/kernel-console": "^6.0.0",
    "psr/log": "^3.0.0",
    "psr/clock": "^1.0.0",
    "psr/http-message": "^2.0.0",
    "psr/http-server-handler": "^1.0.2",
    "psr/http-server-middleware": "^1.0.2",
    "symfony/cache": "^6.4.4||^7.0.4",
    "symfony/routing": "^6.4.5||^7.0.5",
    "symfony/finder": "^6.4.0||^7.0.0",
    "twig/twig": "^3.8.0",
    "twig/markdown-extra": "^3.8.0"
  },

  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.52.0",
    "maglnet/composer-require-checker": "^4.7.1",
    "phpstan/phpstan": "^1.10.63",
    "phpstan/phpstan-phpunit": "^1.3.16",
    "phpunit/phpcov": "^9.0.2",
    "phpunit/phpunit": "^10.5.13"
  }
}
